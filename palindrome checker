class Solution:
    def isPalindrome(self,x:int) -> bool:
        sum = 0
        if x >= 0:
            i = x
            c=1
            b=0
            while i > 0:
                b = i % 10
                sum = sum*10 + b
                i = i // 10
                c=c*10
            return sum == x
        else:
            return sum == x
